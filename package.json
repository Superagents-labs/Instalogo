    "agenda": "^5.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.9.0",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "image-size": "^2.0.2",
    "ioredis": "^5.6.1",
