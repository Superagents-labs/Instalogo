  "version": "1.0.0",
  "description": "Telegram bot for generating business logos using OpenAI",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm install --force",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "start:all": "./start-all-services.sh",
    "stop:all": "./stop-all-services.sh",
    "restart:all": "./stop-all-services.sh && ./start-all-services.sh"
